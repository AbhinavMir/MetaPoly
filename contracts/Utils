pragma solidity ^0.8.11;

struct Property {
    address owner;
    string name;
    string description;
    uint256 id;
    uint256 price;
    uint8 cell;
    uint256 houses; 
}

struct Player {
    address player;
    uint256 balance;
    uint256 position;
    uint256[] properties;
}

interface Property
{
    function sellProperty(uint256 _id, uint256 _price) external;
}